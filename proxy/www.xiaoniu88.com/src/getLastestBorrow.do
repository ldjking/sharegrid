<script type="text/javascript">
	$(".repaymentIcon").poshytip({alignTo: 'target',alignX: 'inner-left',alignY: 'bottom',offsetX:-50,offsetY: 15});
	$(".moreReward").poshytip({alignTo: 'target',alignX: 'inner-left',alignY: 'bottom',offsetX:-10,offsetY: 15});
	$(".transfer").poshytip({alignTo: 'target',alignX: 'inner-left',alignY: 'bottom',offsetX:-10,offsetY: 15});
	$(".isCustom").poshytip({alignTo: 'target',alignX: 'inner-left',alignY: 'bottom',offsetX:-10,offsetY: 15});
	$(function(){
		count_remain_time();
		window.setInterval(function() {
				count_remain_time();
			},1000);
		});
		function count_remain_time() {
			$(".check").each(function() {
			var left_time_int = $(this).attr("left_time_int");
			var bid = $(this).attr("bid");
			if (left_time_int >= 0) {
				var time_string = counterClock(left_time_int)+"后<br/>开始抢购";
				if (left_time_int == 0) {
					$(this).html('<a href="financeDetail.do?id='+bid+'" class="fllbtn fllbtn2">购买</a>');
					var indexbid = $("#hqbbuybtn").attr("bid");
					if(indexbid == bid){
						$("#hqbbuybtn").html("<a target='_blank' href='financeDetail.do?id="+bid+"'>购买</a>");
					}
				} else {
					$(this).html(time_string);
					$(this).attr("left_time_int", left_time_int - 1);
				}
			}  
		  });
	}
</script>